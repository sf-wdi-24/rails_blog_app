<div class="col-sm-10 col-sm-offset-1">
	<h1 class="text-center">Welcome to Rails Blog</h1>
	<hr>
	<div class="row">
		<% @users.each do |user| %>
			<div class="col-sm-8">
	  		<p><strong>Username:</strong> <%= user.name %></p>
			</div>
			<div class="col-sm-4">
				<%= link_to "View Profile",user_path(user), method: :get %>
			</div>
		<% end %>
	</div>
</div>
